{
	"title": "学习笔记",
	"language": "zh-hans",
	"plugins" : [
		"expandable-chapters",
		"chapter-fold",
		"-copy-code-button",
		"code",
		"page-footer-ex",
		"-anchor-navigation-ex",
		"expandable-chapters-small",
		"prism", 
		"-highlight",
		"-lunr", 
		"search", 
		"-search-pro",
		"splitter",
		"-sharing",
		"-insert-logo",
		"hide-element",
		"-page-treeview",
		"sectionx",
		"-popup",
		"-lightbox",
		"theme-fexa",
		"auto-scroll-table"
	],

	"variables": {
		"themeFexa": {
			"nav": [
				{
					"url": "/",
					"target": "_blank",
					"name": "简易教程"
				}
			]
		}
	},
	"styles": {
		"website": "./styles/website.css"
	},

	"pluginsConfig": {
		"page-footer-ex": {
            "copyright": "By Wuyuexin",
            "markdown": true,
            "update_label": "<i>updated</i>",
            "update_format": "YYYY-MM-DD HH:mm:ss"
		},	
		"prism": {
			"css": ["prismjs/themes/prism-okaidia.css"],
			"lang": {"flow": "typescript"}
		},	
		"fontSettings": {
            "family": "Microsoft YaHei",
            "size": 12
        },
		"insert-logo": {
			"url": "http://m.qpic.cn/psc?/V13YQnla16OHe5/ruAMsa53pVQWN7FLK88i5kKcQMt1TjPDJWvATh8Q8zdyGoOoiZM8PpgU5LdzaUMV9vnrFV*fdGOp0CjXtZuHCsj9wE3P19RWf1tJFukKkws!/b&bo=bgIsAQAAAAADB2M!&rf=viewer_4",
			"style": "background: none; max-height: 80px; min-height: 80px"
    	},
		"hide-element":{
			"elements":[".gitbook-link"]
		},
		"anchor-navigation-ex": {
			"showLevel": false,
			"showGoTop": false
		},
		"page-treeview": {
			"copyright": "",
			"minHeaderCount": "2",
			"minHeaderDeep": "2"
		},
		"sectionx": {
			"tag": "b"
		},
		"theme-fexa": {
			"search-placeholder": "输入关键字搜索",
			"logo": "./image/logo.png",
			"favicon": "./favicon.ico"
		},
		"code": {
			"copyButtons": false
		}

	},
	"ignores" : ["_book", "node_modules"]
}